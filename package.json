{
  "name": "ryc",
  "version": "0.2.4",
  "description": "Real Fish Youtube Video Crawling Module",
  "main": "libs/bundle.js",
  "types": "libs/bundle.d.ts",
  "scripts": {
    "test": "ts-node ./node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "dev": "nodemon --watch src --delay 1 --exec 'ts-node' ./src/rundev.ts",
    "cpy:prod": "./node_modules/.bin/cpy ./dist/prod/bundle.d.ts ./libs/ & ./node_modules/.bin/cpy ./dist/prod/bundle.js ./libs/",
    "release": "npm run cpy:prod",
    "build:dev": "webpack --config webpack-config/webpack.dev.config.js",
    "build:prod": "webpack --config webpack-config/webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RealFishDT/RealFishYoutubeCrawling.git"
  },
  "keywords": [
    "youtube",
    "video",
    "comment",
    "crawling",
    "ajax",
    "json",
    "extract",
    "fast"
  ],
  "author": {
    "name": "Realfish",
    "email": "contentdevsolution@gmail.com",
    "url": "https://realfish-likeview.web.app/"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jasmine": "^3.6.9",
    "cpy-cli": "^3.1.1",
    "dts-bundle-webpack": "^1.0.2",
    "gts": "^3.1.0",
    "jasmine": "^3.7.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
